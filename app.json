{
  "expo": {
    "name": "BrainHeros",
    "slug": "BrainHeros",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/brainHerosLogo.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/brainHerosLogo.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.angie0202.BrainHeros"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/brainHerosLogo.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "bf275ebf-b54c-4d3a-9337-438622fd92f6"
      }
    }
  }
}
